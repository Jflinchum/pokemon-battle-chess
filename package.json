    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@pkmn/client": "^0.6.32",
    "@pkmn/data": "^0.9.32",
    "@pkmn/dex": "^0.9.32",
    "@pkmn/img": "^0.2.37",
    "@pkmn/mods": "^0.9.32",
    "@pkmn/protocol": "^0.6.32",
    "@pkmn/randoms": "^0.9.32",
    "@pkmn/view": "^0.6.32",
    "@socket.io/admin-ui": "^0.5.1",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.13",
    "@types/react": "^19.0.10",
