    "react-dom": "^19.0.0",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.28.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "socketio": "^1.0.0",
    "typescript": "~5.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
