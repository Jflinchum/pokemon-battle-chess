{
  "name": "pokemon-chess",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "tsx ./server/index.ts",
    "server:debug": "tsx --inspect-brk ./server/index.ts",
    "build-client": "tsc -b src && vite build",
    "build-server": "tsc -b server",
    "lint": "eslint .",
    "preview": "vite preview",
    "postinstall": "patch-package",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){ process.exit(1)} \" || husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
