{
  "name": "pokemon-chess",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "client": "vite",
    "client:docker": "vite --host=0.0.0.0",
    "lobbyService": "tsx ./lobbyServer/index.ts",
    "lobbyService:debug": "tsx --inspect-brk ./lobbyServer/index.ts",
    "lobbyService:watch": "tsx watch ./lobbyServer/index.ts",
    "lobbyService:docker:debug": "tsx watch --inspect-brk --inspect-port=0.0.0.0:9229 ./lobbyServer/index.ts",
    "gameService": "tsx ./gameServer/index.ts",
    "gameService:debug": "tsx --inspect-brk ./gameServer/index.ts",
    "gameService:watch": "tsx watch ./gameServer/index.ts",
    "gameService:docker:debug": "tsx watch --inspect-brk --inspect-port=0.0.0.0:9230 ./gameServer/index.ts",
    "build-client": "tsc -b src && vite build",
    "build-lobby": "tsc -b lobbyServer",
    "build-game": "tsc -b gameServer",
    "lint": "eslint .",
    "preview": "vite preview",
    "postinstall": "patch-package",
    "injectData": "node util/dev/dataInjection.js",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){ process.exit(1)} \" || husky",
    "test": "vitest --silent",
    "test:coverage": "vitest --coverage --silent"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@pkmn/client": "^0.6.32",
    "@pkmn/data": "^0.9.32",
    "@pkmn/dex": "^0.9.32",
    "@pkmn/img": "^0.2.37",
    "@pkmn/mods": "^0.9.32",
    "@pkmn/protocol": "^0.6.32",
    "@pkmn/randoms": "^0.9.32",
    "@pkmn/view": "^0.6.32",
    "@sesamecare-oss/redlock": "^1.4.0",
    "@socket.io/admin-ui": "^0.5.1",
    "@socket.io/redis-streams-adapter": "^0.2.2",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "bcryptjs": "^3.0.2",
    "chess.js": "1.2.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "ioredis": "^5.6.1",
    "obscenity": "^0.4.3",
    "patch-package": "^8.0.0",
    "react": "^19",
    "react-dom": "^19",
    "react-markdown": "^10.1.0",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.28.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "socketio": "^1.0.0",
    "typescript": "~5.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.0",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsx": "^4.19.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  }
}
